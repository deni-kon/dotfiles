set nocompatible
filetype on
filetype plugin on
filetype indent on
syntax on

set cursorline
set number
set relativenumber

set tabstop=4
set shiftwidth=4
set expandtab

set scrolloff=10

set nowrap

set ignorecase

set showcmd

set showmatch

set hlsearch

set history=1000

set wildmenu

set wildmode=list:longest
